<volist name='data' id='vo' >
    <div class="col-sm-6 border">
        <div class="box">
            <div class="video-container">
                <iframe width="100%" height="100%" src="http://player.youku.com/embed/{$vo.song_ukid}" frameborder="0" allowfullscreen=""></iframe>
                <a class="playbutton"></a>
                <img src="{$vo.song_img}" onerror="javascript:this.src='__PUBLIC__/images/public/default.gif'" class="img-responsive imgradius"/>
            </div>
            <h1>{$vo.song_name}</h1>
            <p class="describe">{$vo.song_introduce}</p>
        </div>
    </div>
</volist>