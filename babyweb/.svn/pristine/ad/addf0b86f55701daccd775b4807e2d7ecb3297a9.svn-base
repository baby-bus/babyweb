<script>
  $(function(){
  	web_url = '{:WEB_URL}';
  	var model = '{:MODULE_NAME}';
  	$(".navbar-nav li").each(function(){
  		var val = $(this).attr("data-value");
  		if(val!="undefined"){
  			if(model==val){
  				$(this).addClass("active");
  				$(this).siblings().removeClass("active");
  			}
  		}
  	})
  })
</script>